<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<h1>Products</h1>

<%= link_to "새 상품", new_product_path if authenticated? %>

<div id="products">
  <% @products.each do |product| %>
    <div>
      <a href="/products/<%= product.id %>">
        <%= product.name %>
      </a>
    </div>
  <% end %>
</div>

<%= debug @products %>
